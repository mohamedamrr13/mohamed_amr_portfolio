{
    "plugins": [
        "@nitedo/vercel-flutter"
    ],
    "buildCommand": "flutter build web",
    "outputDirectory": "build",
    "installCommand": "flutter pub get",
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/.*",
            "dest": "/index.html"
        }
    ]
}